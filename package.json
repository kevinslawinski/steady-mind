{
  "name": "steady-mind",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:no-watch": "ng test --watch=false",
    "build-gh": "ng build --base-href https://kevinslawinski.github.io/steady-mind/",
    "deploy-gh": "npx angular-cli-ghpages --dir=dist/steady-mind/browser --no-silent"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/platform-browser": "^21.0.3",
    "@angular/router": "^21.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.9.0",
    "jsdom": "^27.2.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.15"
  }
}
